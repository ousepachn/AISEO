{
  "name": "functions",
  "description": "Cloud Functions for Firebase",
  "scripts": {
    "lint": "eslint .",
    "serve": "GOOGLE_CLOUD_PROJECT=aiseo-ff704 GOOGLE_CLOUD_LOCATION=global GOOGLE_GENAI_USE_VERTEXAI=true NODE_ENV=local firebase emulators:start --only functions",
    "serve:dev": "NODE_ENV=development firebase emulators:start --only functions",
    "serve:test": "NODE_ENV=test firebase emulators:start --only functions",
    "serve:prod": "NODE_ENV=production firebase emulators:start --only functions",
    "shell": "firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only functions",
    "deploy:dev": "NODE_ENV=development firebase deploy --only functions",
    "deploy:test": "NODE_ENV=test firebase deploy --only functions",
    "deploy:prod": "NODE_ENV=production firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "18"
  },
  "main": "src/index.js",
  "dependencies": {
    "@google-cloud/pubsub": "^5.0.0",
    "@google/genai": "^1.4.0",
    "axios": "^1.6.7",
    "cheerio": "^1.0.0-rc.12",
    "dotenv": "^16.5.0",
    "firebase-admin": "^12.0.0",
    "firebase-functions": "^6.3.2"
  },
  "devDependencies": {
    "eslint": "^8.56.0",
    "eslint-config-google": "^0.14.0",
    "firebase-functions-test": "^3.1.0"
  },
  "private": true
}
